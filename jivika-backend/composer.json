{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "asantibanez/livewire-charts": "^3.0",
        "command/custom-command": "dev-main",
        "consoletvs/charts": "6.*",
        "guzzlehttp/guzzle": "^7.2",
        "infyomlabs/laravel-generator": "^5.4",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "*",
        "laravel/tinker": "^2.7",
        "laravel/ui": "*",
        "livewire/livewire": "^3.0@beta",
        "ramsey/uuid": "4.x-dev",
        "razorpay/razorpay": "^2.9",
        "spatie/laravel-permission": "^6.0@dev",
        "spatie/laravel-sluggable": "^3.5",
        "tymon/jwt-auth": "^2.0",
        "yajra/laravel-datatables-buttons": "^9.1",
        "yajra/laravel-datatables-html": "^9.4",
        "yajra/laravel-datatables-oracle": "^10.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.12",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [{
        "type":"vcs",
        "url":"https://gitlab.com/IshaKeyTech/custom-package.git/"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true
}
